{
  "version": 2,
  "buildCommand": "cd angular-app && npm install && npm run build",
  "outputDirectory": "angular-app/dist/angular-app/browser",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.js"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/angular-app/dist/angular-app/browser/index.html"
    }
  ]
}